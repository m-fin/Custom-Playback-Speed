{
    "name": "Custom Playback Speed",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Set playback speed of video in browser to any number.",
    "homepage_url": "https://matthewfinley.dev",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab"
    ]
}