{
    "name": "Custom Playback Speed",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Set playback speed of video in browser to any number.",
    "homepage_url": "https://github.com/m-fin/Custom-Playback-Speed",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "./icons/icon400.png"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png",
        "256": "./icons/icon256.png"
    }
}